        -- Table d'agrégation horaire
        CREATE TABLE IF NOT EXISTS hourly_energy_stats (
            zone VARCHAR(10),
            hour TIMESTAMP,
            avg_production DOUBLE PRECISION,
            avg_consumption DOUBLE PRECISION,
            avg_renewable_pct DOUBLE PRECISION,
            avg_fossil_free_pct DOUBLE PRECISION,
            avg_carbon_intensity DOUBLE PRECISION,
            total_nuclear DOUBLE PRECISION,
            total_wind DOUBLE PRECISION,
            total_solar DOUBLE PRECISION,
            total_hydro DOUBLE PRECISION,
            record_count INTEGER,
            created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
            PRIMARY KEY (zone, hour)
        );
        
        -- Insérer/Mettre à jour les stats des dernières 24h
        INSERT INTO hourly_energy_stats (
            zone, hour, avg_production, avg_consumption,
            avg_renewable_pct, avg_fossil_free_pct, avg_carbon_intensity,
            total_nuclear, total_wind, total_solar, total_hydro, record_count
        )
        SELECT 
            m.zone,
            DATE_TRUNC('hour', m.datetime) as hour,
            AVG(m.production_total) as avg_production,
            AVG(m.consumption_total) as avg_consumption,
            AVG(m.renewable_percentage) as avg_renewable_pct,
            AVG(m.fossil_free_percentage) as avg_fossil_free_pct,
            AVG(c.carbon_intensity) as avg_carbon_intensity,
            SUM(m.nuclear) as total_nuclear,
            SUM(m.wind) as total_wind,
            SUM(m.solar) as total_solar,
            SUM(m.hydro) as total_hydro,
            COUNT(*) as record_count
        FROM mix_energie_table m
        LEFT JOIN carbone_energie_table c 
            ON m.zone = c.zone AND m.datetime = c.datetime
        WHERE m.datetime >= NOW() - INTERVAL '24 hours'
        GROUP BY m.zone, DATE_TRUNC('hour', m.datetime)
        ON CONFLICT (zone, hour) 
